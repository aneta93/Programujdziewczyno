var names = ['Daria', 'Kasia', 'Sylwia', 'Kinga'];
console.log(names [1]);

function sayHello(){
  console.log('hello'+'world');
}
sayHello();

function hurra(){
  console.log('hurra!');
}

function buu(){
  console.log('buu');
}

var a = 7;
var b = 13;

if (b > a) {
  hurra();
}

if (a < b) {
buu();
}

if (a !== b) {
  hurra();
}

